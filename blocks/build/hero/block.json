{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "vh/hero",
  "version": "0.1.0",
  "title": "Hero",
  "category": "widgets",
  "icon": "button",
  "description": "Page hero",
  "supports": {
    "html": false
  },
  "attributes": {
    "badge": {
      "type": "string",
      "default": "Badge text"
    },
    "showBadge": {
      "type": "boolean",
      "default": false
    },
    "title": {
      "type": "string",
      "default": "Page title"
    },
    "text": {
      "type": "string",
      "default": "Page description"
    }
  },
  "textdomain": "vh-hero",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}